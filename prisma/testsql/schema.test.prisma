/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model cbcajachica {
  unidad    String    @default("CYBERLUX") @db.VarChar(10)
  numcaja   Int
  descaja   String    @db.VarChar(100)
  fecregis  DateTime  @default(now()) @db.Date
  fecinicio DateTime  @db.Date
  fecfinal  DateTime  @db.Date
  feccierre DateTime? @db.Date
  montoini  Decimal   @default(0.00) @db.Decimal(15, 2)
  montoing  Decimal   @default(0.00) @db.Decimal(15, 2)
  montoegr  Decimal   @default(0.00) @db.Decimal(15, 2)
  montofin  Decimal   @default(0.00) @db.Decimal(15, 2)
  montocie  Decimal   @default(0.00) @db.Decimal(15, 2)
  moneda    String    @db.VarChar(4)
  montolim  Decimal   @default(0.00) @db.Decimal(15, 2)
  cerrada   Boolean   @default(false)
  anulada   Boolean   @default(false)
  proceso   Boolean   @default(false)
  cajamayor Boolean   @default(false)
  owneruser Int       @default(1)

  @@ignore
}

