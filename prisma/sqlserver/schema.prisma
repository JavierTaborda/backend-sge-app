generator client {
  provider = "prisma-client-js"
  output   = "../../prisma-clients/sqlserver"
}

datasource db {
  provider = "sqlserver"
  url      = env("SQLSERVER_DATABASE_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model documhever {
  tipodoc String @db.Char(4)
  factnum Int
  coalma  String @db.Char(6)
  nrodoc  Int
  nrodvc  Int

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model resumen {
  modulo      String    @db.Char(10)
  empresa     String    @db.Char(10)
  tiporec     Int       @db.SmallInt
  renglon     String    @db.Char(20)
  codcli      String    @db.Char(20)
  clides      String?   @db.Char(100)
  moneda      String    @db.Char(6)
  monto       Decimal   @db.Decimal(18, 2)
  actualizado DateTime  @db.DateTime
  codalma     String    @db.Char(6)
  orden       Int       @db.SmallInt
  stock_act   Decimal?  @default(0, map: "DF__resumen2__stock___267ABA7A") @db.Decimal(18, 5)
  ult_cos_om  Decimal?  @default(0, map: "DF__resumen2__ult_co__276EDEB3") @db.Decimal(18, 5)
  fec_ult_om  DateTime? @db.Date

  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model resumenOLD {
  modulo      String   @db.Char(10)
  empresa     String   @db.Char(10)
  tiporec     Int      @db.SmallInt
  renglon     String   @db.Char(20)
  codcli      String   @db.Char(20)
  clides      String?  @db.Char(100)
  moneda      String   @db.Char(6)
  monto       Decimal  @db.Decimal(18, 2)
  actualizado DateTime @db.DateTime
  codalma     String   @db.Char(6)
  orden       Int      @db.SmallInt

  @@ignore
}
